splay: flex;\n    align-items: center;\n    justify-content: center;\n}\n.right{\n    flex: 0 0 70%;\n}\n.view-btn{\n  background-color: #016a70;\n  color: #fff;\n  border-color: #016a70;\n  margin-right: 10px;\n}\n.view-btn:hover{\n  border-color: #FFDB64;\n  background-color: #FFDB64;\n  color: #a08843;\n  border-color: #a08843;\n}\n.delete-btn{\n  color: #016a70;\n  border-color: #016a70;\n  margin-right: 10px;\n}\n.delete-btn:hover{\n  background-color: #FA7070;\n  color: #fff;\n  border-color: #FA7070;\n}\n</style>\n"],"mappings":"AA8BA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,MAAM,EAAEC,MAAM;IACdC,EAAE,EAAED,MAAM;IACVH,IAAI,EAAEG,MAAM;IACZE,IAAI,EAAEF,MAAM;IACZG,YAAY,EAAEC,MAAM;IACpBC,KAAK,EAAEL,MAAM;IACbM,GAAG,EAAEN,MAAM;IACXO,MAAM,EAAEH,MAAM;IACdI,IAAI,EAAER;EACR,CAAC;EACDS,IAAI,EAAC,CACH,YAAW,CACZ;EACDC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;IACd;EACF,CAAC;EACDC,OAAO,EAAC;IACN,MAAMC,UAAUA,CAACb,EAAE,EAAC;MAClBc,OAAO,CAACC,GAAG,CAACf,EAAE,CAAC;MACT,IAAI;QACR,MAAMgB,QAAO,GAAI,MAAMrB,KAAK,CAACsB,MAAM,CAAC,eAAe,GAACjB,EAAE,CAAC;QACvDkB,KAAK,CAAC,oBAAoB;QAC1BC,QAAQ,CAACC,MAAM,CAAC,CAAC;QACjB,OAAOJ,QAAQ,CAACP,IAAI;MAEtB,EAAE,OAAOY,KAAK,EAAE;QACdP,OAAO,CAACC,GAAG,CAACM,KAAK,CAAC;MACpB;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAE;IACP,MAAMC,EAAC,GAAIC,YAAY,CAACC,OAAO,CAAC,UAAU;IAC1C,IAAI,CAACf,QAAO,GAAIa,EAAE;EACpB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}