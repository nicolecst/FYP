{"ast":null,"code":"import axios from \"axios\";\nimport ColorTags from \"./ColorTags.vue\";\nexport default {\n  name: \"CardView\",\n  components: {\n    ColorTags\n  },\n  props: {\n    actname: String,\n    area: String,\n    district: String,\n    type: String,\n    category: String,\n    charge: String,\n    act_ID: String,\n    img: String,\n    approved: Boolean\n  },\n  methods: {\n    async deleteAct(aid) {\n      console.log('aid: ' + aid);\n      try {\n        const response = await axios.delete(\"/api/deletAct/\" + aid);\n        alert('Activity Deleted!');\n        location.reload();\n        return response.data;\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","ColorTags","name","components","props","actname","String","area","district","type","category","charge","act_ID","img","approved","Boolean","methods","deleteAct","aid","console","log","response","delete","alert","location","reload","data","error"],"sources":["/Users/nicolechan/Desktop/FYP_Test/itinerary/src/components/CardView.vue"],"sourcesContent":["<template>\n  <div class=\"card\" style=\"min-width: 18rem; max-width: 18rem\">\n    <img\n      class=\"card-img-top\"\n      src=\"@/assets/uploads/victoria_peak.jpg\"\n      alt=\"Card image cap\"\n    />\n    <!-- <img class=\"card-img-top\" :src=\"`http://localhost:3000/${img}`\" alt=\"Card image cap\" /> -->\n    <!-- <img class=\"card-img-top\" src=\"\" alt=\"Card image cap\" /> -->\n    <div class=\"card-body\">\n      <div class=\"card-body\">\n        <h5 class=\"card-title\">{{ actname }}</h5>\n        <!-- <p class=\"card-text\">img: {{ img }}</p> -->\n        <p class=\"card-text\"><ColorTags color=\"#D2E9E9\" :text=\"area\" /></p>\n        <p class=\"card-text\">District: {{ district }}</p>\n        <slot name=\"type\"></slot>\n        <p class=\"card-text\">Category: {{ category }}</p>\n        <p class=\"card-text\">Charge: {{ charge }}</p>\n        <p class=\"card-text\">Approved: {{ approved }}</p>\n        <p class=\"card-text\">ID: {{ act_ID }}</p>\n        <router-link :to=\"'/actDetails/' + act_ID\"\n          ><button class=\"btn btn-primary\">check</button></router-link\n        >\n\n        <!-- button trigger modal -->\n        <button\n              type=\"button\"\n              class=\"btn delete-btn \"\n              @click=\"deleteAct(act_ID)\"\n            >Delete\n            </button>\n\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport ColorTags from \"./ColorTags.vue\";\nexport default {\n  name: \"CardView\",\n  components: {\n    ColorTags,\n  },\n  props: {\n    actname: String,\n    area: String,\n    district: String,\n    type: String,\n    category: String,\n    charge: String,\n    act_ID: String,\n    img: String,\n    approved: Boolean,\n  },\n  methods:{\n    async deleteAct(aid){\n      console.log('aid: '+ aid)\n      try {\n        const response = await axios.delete(\"/api/deletAct/\"+aid);\n        alert('Activity Deleted!')\n        location.reload();\n        return response.data;\n\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.card {\n  margin: 10px;\n  height: auto;\n}\n.btn {\n  background-color: #1fbfaf;\n  color: #fff;\n  border-color: #1fbfaf;\n  margin-right: 10px;\n}\n.btn:hover {\n  border-color: #ffdb64;\n  background-color: #ffdb64;\n  color: #a08843;\n  border-color: #a08843;\n}\n.delete-btn{\n  background-color: #fff;\n  color: #1fbfaf;\n  border-color: #1fbfaf;\n}\n.delete-btn:hover {\n  background-color: #fa7070;\n  color: #fff;\n  border-color: #fa7070;\n}\n\n.cancel-button{\n  background-color: #fff;\n  color: #1fbfaf;\n  border-color: #1fbfaf;\n}\n.modalDelete-btn:hover{\n  background-color: #fa7070;\n  color: #fff;\n  border-color: #fa7070;\n}\n</style>\n"],"mappings":"AAsCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,eAAe;EACbC,IAAI,EAAE,UAAU;EACh