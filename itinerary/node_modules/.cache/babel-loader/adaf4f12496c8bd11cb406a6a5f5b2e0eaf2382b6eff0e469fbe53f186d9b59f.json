],\n        [{ itin: { name: 2 } }],\n        [{ itin: { name: 3 } }],\n      ],\n    };\n  },\n  methods: {\n    addDays(date, days) {\n      // Function to add Days\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleString();\n    },\n    weekdays(date, days) {\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleString(\"en-US\", { weekday: \"long\" });\n    },\n    async Rate(){\n    //   const rateData = {\n    //     author: this.history.author,\n    //     name: this.history.name,\n    //     type: this.history.type,\n    //     participants: this.history.participants,\n    //     from: this.history.from,\n    //     to: this.history.to,\n    //     dailyItin: this.dailyItin,\n    //     is_public: this.history.is_public,\n    //     rate: this.rate,\n    //     comment: this.comment\n    //   }\n\n    //   await axios.put('/api/rate/'+iid, rateData).then((response) => {\n    //     console.log(response.data);\n    //     alert(\"Successfully updated!\");\n    // });\n    console.log('rate!')\n  }\n  },\n  mounted() {\n    const fetchData = async () => {\n      const route = useRoute();\n      const response = await axios\n        .get(\"/api/history/\" + route.params.id)\n        .then((response) => (this.history = response.data));\n\n      // Calculate dateDiff based on the first and last items from the history array\n      const start = new Date(this.history.from).getTime();\n      const end = new Date(this.history.to).getTime();\n      const diff = end - start;\n      const dateDiff = Math.floor(diff / 86400000) + 1;\n      this.dateDiff = dateDiff;\n      console.log(response);\n      console.log(\"length: \" + this.history.dailyItin.length);\n      console.log(this.history.dailyItin[2]);\n\n      this.itinArray = this.history.dailyItin;\n      console.log(\"length2: \" + this.itinArray.length);\n      console.log(this.history.dailyItin[0]);\n    };\n    fetchData();\n  },\n  setup() {\n    const rate = ref();\n    const comment = ref(\"\");\n\n    const popupTriggers = ref({\n      buttonTrigger: false,\n    });\n\n    const togglePopup = (trigger) => {\n      popupTriggers.value[trigger] = !popupTriggers.value[trigger];\n    };\n\n    return {\n      popupTriggers,\n      togglePopup,\n      rate,\n      comment,\n    }\n  },\n};\n</script>\n<style scoped>\n.rate-btn{\n  border-color: #016a70;\n  color: #016a70;\n}\n.rate-btn :hover{\n  border-color: #016a70;\n  background-color: #016a70;\n  color: #ffffdd;\n}\n.scrolls {\n  display: flex;\n  flex-wrap: no-wrap;\n  overflow-x: auto;\n  margin: 20px;\n}\n</style>\n"],"mappings":"AAkDA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,YAAW,MAAO,4BAA4B;AACrD,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVR,MAAM;IACNC,QAAQ;IACRC,WAAW;IACXC;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,CAAC;MACXC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE,CACT,CACE;QAAEC,IAAI,EAAE;UAAER,IAAI,EAAE;QAAE;MAAE,CAAC,EACrB;QAAEQ,IAAI,EAAE;UAAER,IAAI,EAAE;QAAI;MAAE,CAAC,EACvB;QAAEQ,IAAI,EAAE;UAAER,IAAI,EAAE;QAAI;MAAE,CAAC,CACxB,EACD,CAAC;QAAEQ,IAAI,EAAE;UAAER,IAAI,EAAE;QAAE;MAAE,CAAC,CAAC,EACvB,CAAC;QAAEQ,IAAI,EAAE;UAAER,IAAI,EAAE;QAAE;MAAE,CAAC,CAAC;IAE3B,CAAC;EACH,CAAC;EACDS,OAAO,EAAE;IACPC,OAAOA,CAACC,IAAI,EAAEC,IAAI,EAAE;MAClB;MACA,IAAIC,MAAK,GAAI,IAAIC,IAAI,CAACH,IAAI,CAAC;MAC3BE,MAAM,CAACE,OAAO,CAACF,MAAM,CAACG,OAAO,CAAC,IAAIJ,IAAI,CAAC;MACvC,OAAOC,MAAM,CAACI,cAAc,CAAC,CAAC;IAChC,CAAC;IACDC,QAAQA,CAACP,IAAI,EAAEC,IAAI,EAAE;MACnB,IAAIC,MAAK,GAAI,IAAIC,IAAI,CAACH,IAAI,CAAC;MAC3BE,MAAM,CAACE,OAAO,CAACF,MAAM,CAACG,OAAO,CAAC,IAAIJ,IAAI,CAAC;MACvC,OAAOC,MAAM,CAACI,cAAc,CAAC,OAAO,EAAE;QAAEE,OAAO,EAAE;MAAO,CAAC,CAAC;IAC5D,CAAC;IACD,MAAMC,IAAIA,CAAA,EAAE;MACZ;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;