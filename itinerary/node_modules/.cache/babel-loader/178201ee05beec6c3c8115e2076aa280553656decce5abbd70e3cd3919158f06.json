{"ast":null,"code":"import axios from \"axios\";\nimport NavBarVue from \"../components/NavBar.vue\";\n// import ActivityCardVue from '../components/ActivityCard.vue';\nimport CardViewVue from '../components/CardView.vue';\nexport default {\n  name: \"HomeView\",\n  components: {\n    NavBarVue,\n    // ActivityCardVue,\n    CardViewVue\n  },\n  data() {\n    return {\n      activities: []\n    };\n  },\n  methods: {\n    actCat(cat) {\n      return this.activities.filter(a => a.Category === cat);\n    },\n    approved() {\n      return this.activities.filter(a => a.Approved === 'true');\n    }\n  },\n  async created() {\n    const response = await axios.get(\"/api/\").then(response => this.activities = response.data);\n\n    // const activities = response.data;\n    // this.activities = activities;\n\n    console.log(response);\n  }\n};","map":{"version":3,"names":["axios","NavBarVue","CardViewVue","name","components","data","activities","methods","actCat","cat","filter","a","Category","approved","Approved","created","response","get","then","console","log"],"sources":["/Users/nicolechan/Desktop/FYP_Test/itinerary/src/pages/HomeView.vue"],"sourcesContent":["<template>\n  <div>\n    <NavBarVue />\n    <h3>All activities</h3>\n    <div class=\"scrolls\">\n      <CardViewVue v-for=\"activity in approved().slice(0,10)\"\n        :key=\"activity._id\" :actname=\" activity.Act_name\" :area=\"activity.Area\" :district=\"activity.District\" :type=\"activity.Type\" :category = \"activity.Category\" :charge=\"activity.Charge\"/>\n    </div>\n    <h3>Wellness and Retreats</h3>\n    <div class=\"scrolls\">\n      <CardViewVue v-for=\"activity in actCat('Wellness and Retreats')\"\n        :key=\"activity._id\" :actname=\" activity.Act_name\" :area=\"activity.Area\" :district=\"activity.District\" :type=\"activity.Type\" :category = \"activity.Category\" :charge=\"activity.Charge\"/>\n    </div>\n    \n    <!-- <ActivityCardVue/> -->\n\n  </div>\n</template>\n\n<script type=\"module\">\nimport axios from \"axios\";\nimport NavBarVue from \"../components/NavBar.vue\";\n// import ActivityCardVue from '../components/ActivityCard.vue';\nimport CardViewVue from '../components/CardView.vue';\n\nexport default {\n  name: \"HomeView\",\n  components: {\n    NavBarVue,\n    // ActivityCardVue,\n    CardViewVue\n  },\n  data() {\n    return {\n      activities: [],\n    };\n  },\n  methods:{\n    actCat(cat){\n        return this.activities.filter((a)=> a.Category === cat)\n    },\n    approved(){\n      return this.activities.filter((a)=>a.Approved === 'true')\n    }\n  },\n  async created() {\n    const response = await axios\n      .get(\"/api/\")\n      .then((response) => (this.activities = response.data));\n\n    // const activities = response.data;\n    // this.activities = activities;\n\n    console.log(response);\n  },\n};\n</script>\n\n<style>\n.scrolls {\n  display: flex;\n  flex-wrap: no-wrap;\n  overflow-x: auto;\n  margin: 20px;\n}\n\n</style>\n"],"mappings":"AAoBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,0BAA0B;AAChD;AACA,OAAOC,WAAU,MAAO,4BAA4B;AAEpD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVH,SAAS;IACT;IACAC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAC;IACNC,MAAMA,CAACC,GAAG,EAAC;MACP,OAAO,IAAI,CAACH,UAAU,CAACI,MAAM,CAAEC,CAAC,IAAIA,CAAC,CAACC,QAAO,KAAMH,GAAG;IAC1D,CAAC;IACDI,QAAQA,CAAA,EAAE;MACR,OAAO,IAAI,CAACP,UAAU,CAACI,MAAM,CAAEC,CAAC,IAAGA,CAAC,CAACG,QAAO,KAAM,MAAM;IAC1D;EACF,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,QAAO,GAAI,MAAMhB,KAAI,CACxBiB,GAAG,CAAC,OAAO,EACXC,IAAI,CAAEF,QAAQ,IAAM,IAAI,CAACV,UAAS,GAAIU,QAAQ,CAACX,IAAK,CAAC;;IAExD;IACA;;IAEAc,OAAO,CAACC,GAAG,CAACJ,QAAQ,CAAC;EACvB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}