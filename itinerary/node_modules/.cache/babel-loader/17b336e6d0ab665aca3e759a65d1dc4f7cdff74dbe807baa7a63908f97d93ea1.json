{"ast":null,"code":"import NavBar from \"@/components/NavBar.vue\";\nimport HistCard from \"@/components/HistCard.vue\";\nimport axios from \"axios\";\nimport { ref } from 'vue';\nexport default {\n  name: \"AllHistory\",\n  components: {\n    NavBar,\n    HistCard\n  },\n  data() {\n    return {\n      plans: []\n    };\n  },\n  async created() {\n    const i = localStorage.getItem(\"userID\");\n    console.log(i);\n    const response = axios.get(\"/api/itinHistory\").then(response => this.plans = response.data.filter(a => a.is_public === true));\n    console.log(response);\n  },\n  setup() {\n    const filterShow = ref(false);\n    const checkedType = ref([]);\n    return {\n      filterShow,\n      checkedType\n    };\n  }\n};","map":{"version":3,"names":["NavBar","HistCard","axios","ref","name","components","data","plans","created","i","localStorage","getItem","console","log","response","get","then","filter","a","is_public","setup","filterShow","checkedType"],"sources":["/Users/nicolechan/Desktop/FYP_Test/itinerary/src/pages/AllHistory.vue"],"sourcesContent":["<template lang=\"\">\n  <div>\n    <NavBar />\n    <div class=\"container\">\n      <nav style=\"--bs-breadcrumb-divider: '>'\" aria-label=\"breadcrumb\">\n        <ol class=\"breadcrumb\">\n          <li class=\"breadcrumb-item\">\n            <a href=\"/\">Home</a>\n          </li>\n          <li class=\"breadcrumb-item active\" aria-current=\"page\">\n            Itinerary References\n          </li>\n        </ol>\n      </nav>\n      <div class=\"sub-container\">\n        <h1 style=\"font-family: BungeeInline\">Itinerary References</h1>\n\n        <div class=\"row mt-4\">\n          <button class=\"btn btn-primary\" @click=\"filterShow = !filterShow\">Filter</button>\n          <div class=\"row\" v-if=\"filterShow\">\n            <div class=\"filter-form\">\n              <div class=\"checkboxType\">\n                <div><h4>Type</h4></div>\n                <div><input\n                  type=\"checkbox\"\n                  id=\"solo\"\n                  value=\"Solo\"\n                  v-model=\"checkedType\"\n                />\n                <label for=\"solo\">Solo</label></div>\n                \n                <div><input\n                  type=\"checkbox\"\n                  id=\"couple\"\n                  value=\"Couple\"\n                  v-model=\"checkedType\"\n                />\n                <label for=\"couple\">Couple</label></div>\n                \n                <div><input\n                  type=\"checkbox\"\n                  id=\"family\"\n                  value=\"Family\"\n                  v-model=\"checkedType\"\n                />\n                <label for=\"family\">Family</label></div>\n                \n                <div><input\n                  type=\"checkbox\"\n                  id=\"friends\"\n                  value=\"Friends\"\n                  v-model=\"checkedType\"\n                />\n                <label for=\"friends\">Friends</label></div>\n                <p>{{checkedType}}</p>\n                \n              </div>\n            </div>\n          </div>\n        </div>\n\n        <HistCard\n          v-for=\"plan in plans\"\n          :key=\"plan._id\"\n          :author=\"plan.authorName\"\n          :id=\"plan._id\"\n          :name=\"plan.name\"\n          :type=\"plan.type\"\n          :participants=\"plan.participants\"\n          :start=\"plan.from\"\n          :end=\"plan.to\"\n          :rating=\"plan.rate\"\n          :view=\"plan.is_public\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport NavBar from \"@/components/NavBar.vue\";\nimport HistCard from \"@/components/HistCard.vue\";\nimport axios from \"axios\";\nimport { ref } from 'vue';\n\nexport default {\n  name: \"AllHistory\",\n  components: {\n    NavBar,\n    HistCard,\n  },\n  data() {\n    return {\n      plans: [],\n    };\n  },\n  async created() {\n    const i = localStorage.getItem(\"userID\");\n    console.log(i);\n\n    const response = axios\n      .get(\"/api/itinHistory\")\n      .then(\n        (response) =>\n          (this.plans = response.data.filter((a) => a.is_public === true))\n      );\n\n    console.log(response);\n  },\n  setup(){\n    const filterShow = ref(false);\n    const checkedType = ref([]);\n\n    return{\n      filterShow,\n      checkedType\n    }\n  }\n};\n</script>\n\n<style scoped>\n.container {\n  margin-top: 30px;\n}\n\n.sub-container {\n  display: flex;\n  flex-wrap: wrap;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  background-color: #ffefbb;\n  width: 100%;\n  padding: 30px;\n}\n.filter-form {\n  background-color: #fff;\n  position: absolute;\n  height: 300px;\n  width: 300px;\n  z-index: 100;\n  border-radius: 10px;\n  box-shadow: 0.3em 0.3em 1em #d3d3d3;;\n}\n.checkboxType{\n  display: flex;\n  flex-wrap: wrap;\n  flex-direction: column;\n  width: 100%;\n}\n</style>\n"],"mappings":"AAgFA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVL,MAAM;IACNC;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,CAAA,GAAIC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC;IACxCC,OAAO,CAACC,GAAG,CAACJ,CAAC,CAAC;IAEd,MAAMK,QAAO,GAAIZ,KAAI,CAClBa,GAAG,CAAC,kBAAkB,EACtBC,IAAI,CACFF,QAAQ,IACN,IAAI,CAACP,KAAI,GAAIO,QAAQ,CAACR,IAAI,CAACW,MAAM,CAAEC,CAAC,IAAKA,CAAC,CAACC,SAAQ,KAAM,IAAI,CAClE,CAAC;IAEHP,OAAO,CAACC,GAAG,CAACC,QAAQ,CAAC;EACvB,CAAC;EACDM,KAAKA,CAAA,EAAE;IACL,MAAMC,UAAS,GAAIlB,GAAG,CAAC,KAAK,CAAC;IAC7B,MAAMmB,WAAU,GAAInB,GAAG,CAAC,EAAE,CAAC;IAE3B,OAAM;MACJkB,UAAU;MACVC;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}