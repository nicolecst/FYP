{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavBar from \"@/components/NavBar.vue\";\nimport PlanCard from \"@/components/PlanCard.vue\";\nimport SubplanCard from \"@/components/SubplanCard.vue\";\nimport PopupRateVue from \"@/components/PopupRate.vue\";\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nimport { ref } from \"vue\";\nexport default {\n  name: \"itinhistDetails\",\n  components: {\n    NavBar,\n    PlanCard,\n    SubplanCard,\n    PopupRateVue\n  },\n  data() {\n    return {\n      history: [],\n      r: 0,\n      dateDiff: 0,\n      itinArray: [],\n      itinSubArray: []\n    };\n  },\n  methods: {\n    addDays(date, days) {\n      // Function to add Days\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleDateString();\n    },\n    weekdays(date, days) {\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleString(\"en-US\", {\n        weekday: \"long\"\n      });\n    },\n    ratingUpdate(rating) {\n      this.r = rating;\n    },\n    sendEmail() {\n      const i = localStorage.getItem(\"userID\");\n      console.log(i);\n      const e = localStorage.getItem(\"email\");\n      console.log(e);\n      axios.get(\"/api/sendItin\", {\n        params: {\n          message: JSON.stringify(this.itinArray),\n          email: e\n        }\n      }).then(response => console.log(response));\n      alert(\"Itinerary Sent!\");\n      this.$router.push('/');\n    },\n    editItin(itinID) {\n      this.$router.push('/itinEdit/' + itinID);\n    },\n    async Rate(iid) {\n      const rateData = {\n        author: this.history.author,\n        authorName: this.history.authorName,\n        name: this.history.name,\n        type: this.history.type,\n        participants: this.history.participants,\n        from: this.history.from,\n        to: this.history.to,\n        dailyItin: this.history.dailyItin,\n        is_public: this.history.is_public,\n        selectedRate: this.r,\n        comment: this.comment\n      };\n      await axios.put(\"/api/rate/\" + iid, rateData).then(response => {\n        console.log(response.data);\n        alert(\"Successfully updated!\");\n      });\n      console.log(rateData);\n    }\n  },\n  mounted() {\n    const fetchData = async () => {\n      const route = useRoute();\n      const response = await axios.get(\"/api/history/\" + route.params.id).then(response => this.history = response.data);\n\n      // Calculate dateDiff based on the first and last items from the history array\n      const start = new Date(this.history.from).getTime();\n      const end = new Date(this.history.to).getTime();\n      const diff = end - start;\n      const dateDiff = Math.floor(diff / 86400000) + 1;\n      this.dateDiff = dateDiff;\n      console.log(response);\n      console.log(\"length: \" + this.history.dailyItin.length);\n      console.log(this.history.dailyItin[2]);\n      this.itinArray = this.history.dailyItin;\n      console.log(\"length2: \" + this.itinArray.length);\n      console.log(this.history.dailyItin[0]);\n    };\n    fetchData();\n  },\n  setup() {\n    const comment = ref(\"\");\n    const uid = localStorage.getItem(\"userID\");\n    const popupTriggers = ref({\n      buttonTrigger: false\n    });\n    const togglePopup = trigger => {\n      popupTriggers.value[trigger] = !popupTriggers.value[trigger];\n    };\n    return {\n      popupTriggers,\n      togglePopup,\n      comment,\n      uid\n    };\n  }\n};","map":{"version":3,"names":["NavBar","PlanCard","SubplanCard","PopupRateVue","axios","useRoute","ref","name","components","data","history","r","dateDiff","itinArray","itinSubArray","methods","addDays","date","days","result","Date","setDate","getDate","toLocaleDateString","weekdays","toLocaleString","weekday","ratingUpdate","rating","sendEmail","i","localStorage","getItem","console","log","e","get","params","message","JSON","stringify","email","then","response","alert","$router","push","editItin","itinID","Rate","iid","rateData","author","authorName","type","participants","from","to","dailyItin","is_public","selectedRate","comment","put","mounted","fetchData","route","id","start","getTime","end","diff","Math","floor","length","setup","uid","popupTriggers","buttonTrigger","togglePopup","trigger","value"],"sources":["/Users/nicolechan/Desktop/FYP_Test/itinerary/src/pages/ItinhistDetails.vue"],"sourcesContent":["<template>\n  <div>\n    <NavBar />\n    <div class=\"container\">\n      <nav style=\"--bs-breadcrumb-divider: '>'\" aria-label=\"breadcrumb\">\n        <ol class=\"breadcrumb\">\n          <li class=\"breadcrumb-item\">\n            <a href=\"/\">Home</a>\n          </li>\n          <li class=\"breadcrumb-item\">\n            <a href=\"/history\">Itinerary History</a>\n          </li>\n          <li class=\"breadcrumb-item active\" aria-current=\"page\">Details</li>\n        </ol>\n      </nav>\n\n      <!-- <h1>Itinerary History Details</h1> -->\n      <div><input type=\"checkbox\" v-model=\"this.history.is_public\">Public</div>\n      <div class=\"scrolls\">\n        <PlanCard\n          v-for=\"(n, i) in dateDiff\"\n          :key=\"n\"\n          :n=\"n\"\n          :start=\"addDays(this.history.from, i)\"\n          :daysOfWeek=\"weekdays(this.history.from, i)\"\n        >\n          <div v-for=\"(row, rowIndex) in itinArray[i]\" :key=\"rowIndex\">\n            <div v-for=\"(element, columnIndex) in row\" :key=\"columnIndex\">\n              <SubplanCard\n                :actName=\"element.act_name\"\n                :location=\"element.location\"\n                :sTime=\"element.startTime\"\n                :eTime=\"element.endTime\"\n                :memo=\"element.memo\"\n              >\n              </SubplanCard>\n            </div>\n          </div>\n        </PlanCard>\n      </div>\n\n      \n      <div class=\"rate-container\">\n        <button\n          v-if=\"this.history.author == uid\"\n          class=\"btn rate-btn\"\n          style=\"margin-right: 10px; padding-top: 3px; padding-bottom: 3px\"\n          @click.prevent=\"() => togglePopup('buttonTrigger')\"\n        >\n          <span style=\"font-size: 1.5em; margin-right: 5px\">\n            <font-awesome-icon :icon=\"['fas', 'star']\" /> </span\n          >Rate\n        </button>\n        <!-- <button\n          v-if=\"this.history.author == uid\"\n          class=\"btn rate-btn\"\n          style=\"margin-right: 10px; padding-top: 3px; padding-bottom: 3px\"\n          @click.prevent=\"editItin(this.history._id)\"\n        >\n          <span style=\"font-size: 1.5em; margin-right: 5px\">\n            <font-awesome-icon :icon=\"['fas', 'file-export']\" /> </span\n          >Update Itinerary\n        </button> -->\n        <button\n          v-if=\"this.history.author == uid\"\n          class=\"btn rate-btn\"\n          style=\"margin-right: 10px; padding-top: 3px; padding-bottom: 3px\"\n          @click.prevent=\"sendEmail\"\n        >\n          <span style=\"font-size: 1.5em; margin-right: 5px\">\n            <font-awesome-icon :icon=\"['fas', 'file-export']\" /> </span\n          >Get Itinerary\n        </button>\n        <div class=\"row mt-4\">\n          <div>\n            <p\n              v-for=\"n in this.history.selectedRate\"\n              :key=\"n\"\n              style=\"display: inline; margin: 5px\"\n            >\n              <font-awesome-icon :icon=\"['fas', 'star']\" />\n            </p>\n          </div>\n        </div>\n\n        <div class=\"row mt-4\">\n          <p>\n            <span style=\"margin: 5px\"\n              ><font-awesome-icon :icon=\"['fas', 'comment-dots']\" /></span\n            >{{ this.history.comment }}\n          </p>\n        </div>\n      </div>\n\n      <PopupRateVue\n        v-if=\"popupTriggers.buttonTrigger\"\n        :togglePopup=\"() => togglePopup('buttonTrigger')\"\n        v-model:comment=\"comment\"\n        @rating-updated=\"ratingUpdate\"\n        @rate=\"Rate(this.history._id)\"\n      />\n\n      <!-- <p v-for=\"(itin, i) in this.history.dailyItin\" :key=\"i\">\n      {{ itin }}\n    </p> -->\n      <!-- <p>{{ history }}</p> -->\n      <!-- <p>dateDiff: {{ dateDiff }}</p> -->\n      <p></p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport NavBar from \"@/components/NavBar.vue\";\nimport PlanCard from \"@/components/PlanCard.vue\";\nimport SubplanCard from \"@/components/SubplanCard.vue\";\nimport PopupRateVue from \"@/components/PopupRate.vue\";\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nimport { ref } from \"vue\";\n\nexport default {\n  name: \"itinhistDetails\",\n  components: {\n    NavBar,\n    PlanCard,\n    SubplanCard,\n    PopupRateVue,\n  },\n  data() {\n    return {\n      history: [],\n      r: 0,\n      dateDiff: 0,\n      itinArray: [],\n      itinSubArray: [],\n    };\n  },\n  methods: {\n    addDays(date, days) {\n      // Function to add Days\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleDateString();\n    },\n    weekdays(date, days) {\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleString(\"en-US\", { weekday: \"long\" });\n    },\n    ratingUpdate(rating) {\n      this.r = rating;\n    },\n    sendEmail() {\n      const i = localStorage.getItem(\"userID\");\n      console.log(i);\n      const e = localStorage.getItem(\"email\");\n      console.log(e)\n      axios\n        .get(\"/api/sendItin\", { params: { message: JSON.stringify(this.itinArray), email: e } })\n        .then((response) => console.log(response));\n\n        alert(\"Itinerary Sent!\")\n        this.$router.push('/')\n    },\n    editItin(itinID){\n      this.$router.push('/itinEdit/'+ itinID)\n    },\n    async Rate(iid) {\n      const rateData = {\n        author: this.history.author,\n        authorName: this.history.authorName,\n        name: this.history.name,\n        type: this.history.type,\n        participants: this.history.participants,\n        from: this.history.from,\n        to: this.history.to,\n        dailyItin: this.history.dailyItin,\n        is_public: this.history.is_public,\n        selectedRate: this.r,\n        comment: this.comment,\n      };\n\n      await axios.put(\"/api/rate/\" + iid, rateData).then((response) => {\n        console.log(response.data);\n        alert(\"Successfully updated!\");\n      });\n      console.log(rateData);\n    },\n  },\n  mounted() {\n    const fetchData = async () => {\n      const route = useRoute();\n      const response = await axios\n        .get(\"/api/history/\" + route.params.id)\n        .then((response) => (this.history = response.data));\n\n      // Calculate dateDiff based on the first and last items from the history array\n      const start = new Date(this.history.from).getTime();\n      const end = new Date(this.history.to).getTime();\n      const diff = end - start;\n      const dateDiff = Math.floor(diff / 86400000) + 1;\n      this.dateDiff = dateDiff;\n      console.log(response);\n      console.log(\"length: \" + this.history.dailyItin.length);\n      console.log(this.history.dailyItin[2]);\n\n      this.itinArray = this.history.dailyItin;\n      console.log(\"length2: \" + this.itinArray.length);\n      console.log(this.history.dailyItin[0]);\n    };\n    fetchData();\n  },\n  setup() {\n    const comment = ref(\"\");\n    const uid = localStorage.getItem(\"userID\");\n\n    const popupTriggers = ref({\n      buttonTrigger: false,\n    });\n\n    const togglePopup = (trigger) => {\n      popupTriggers.value[trigger] = !popupTriggers.value[trigger];\n    };\n\n    return {\n      popupTriggers,\n      togglePopup,\n      comment,\n      uid,\n    };\n  },\n};\n</script>\n<style scoped>\n.container {\n  margin-top: 30px;\n}\n.rate-btn {\n  border-color: #016a70;\n  color: #016a70;\n}\n.rate-btn :hover {\n  border-color: #016a70;\n  background-color: #016a70;\n  color: #ffffdd;\n}\n.rate-container {\n  margin: 30px;\n  border: 1px solid rgb(0, 0, 0, 0.2);\n  border-radius: 10px;\n}\n.scrolls {\n  display: flex;\n  flex-wrap: no-wrap;\n  overflow-x: auto;\n  margin: 20px;\n}\n</style>\n"],"mappings":";AAiHA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,YAAW,MAAO,4BAA4B;AACrD,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVR,MAAM;IACNC,QAAQ;IACRC,WAAW;IACXC;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,CAAC,EAAE,CAAC;MACJC,QAAQ,EAAE,CAAC;MACXC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,OAAOA,CAACC,IAAI,EAAEC,IAAI,EAAE;MAClB;MACA,IAAIC,MAAK,GAAI,IAAIC,IAAI,CAACH,IAAI,CAAC;MAC3BE,MAAM,CAACE,OAAO,CAACF,MAAM,CAACG,OAAO,CAAC,IAAIJ,IAAI,CAAC;MACvC,OAAOC,MAAM,CAACI,kBAAkB,CAAC,CAAC;IACpC,CAAC;IACDC,QAAQA,CAACP,IAAI,EAAEC,IAAI,EAAE;MACnB,IAAIC,MAAK,GAAI,IAAIC,IAAI,CAACH,IAAI,CAAC;MAC3BE,MAAM,CAACE,OAAO,CAACF,MAAM,CAACG,OAAO,CAAC,IAAIJ,IAAI,CAAC;MACvC,OAAOC,MAAM,CAACM,cAAc,CAAC,OAAO,EAAE;QAAEC,OAAO,EAAE;MAAO,CAAC,CAAC;IAC5D,CAAC;IACDC,YAAYA,CAACC,MAAM,EAAE;MACnB,IAAI,CAACjB,CAAA,GAAIiB,MAAM;IACjB,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,MAAMC,CAAA,GAAIC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC;MACxCC,OAAO,CAACC,GAAG,CAACJ,CAAC,CAAC;MACd,MAAMK,CAAA,GAAIJ,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MACvCC,OAAO,CAACC,GAAG,CAACC,CAAC;MACb/B,KAAI,CACDgC,GAAG,CAAC,eAAe,EAAE;QAAEC,MAAM,EAAE;UAAEC,OAAO,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAAC3B,SAAS,CAAC;UAAE4B,KAAK,EAAEN;QAAE;MAAE,CAAC,EACtFO,IAAI,CAAEC,QAAQ,IAAKV,OAAO,CAACC,GAAG,CAACS,QAAQ,CAAC,CAAC;MAE1CC,KAAK,CAAC,iBAAiB;MACvB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG;IACzB,CAAC;IACDC,QAAQA,CAACC,MAAM,EAAC;MACd,IAAI,CAACH,OAAO,CAACC,IAAI,CAAC,YAAY,GAAEE,MAAM;IACxC,CAAC;IACD,MAAMC,IAAIA,CAACC,GAAG,EAAE;MACd,MAAMC,QAAO,GAAI;QACfC,MAAM,EAAE,IAAI,CAAC1C,OAAO,CAAC0C,MAAM;QAC3BC,UAAU,E