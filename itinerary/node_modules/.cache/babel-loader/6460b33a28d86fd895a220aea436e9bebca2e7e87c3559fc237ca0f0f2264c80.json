{"ast":null,"code":"export default {\n  name: \"HistCard\",\n  props: {\n    author: String,\n    id: String,\n    name: String,\n    type: String,\n    participants: Number,\n    start: String,\n    end: String,\n    rating: Number,\n    view: String\n  },\n  emit: [\"deleteItin\"],\n  data() {\n    return {\n      userName: \"\",\n      selectedId: null\n    };\n  },\n  methods: {\n    setSelectedId(id) {\n      alert(id);\n      this.setSelectedId = id;\n      alert(this.selectedId);\n    }\n  },\n  mounted() {\n    const un = localStorage.getItem(\"username\");\n    this.userName = un;\n  }\n};","map":{"version":3,"names":["name","props","author","String","id","type","participants","Number","start","end","rating","view","emit","data","userName","selectedId","methods","setSelectedId","alert","mounted","un","localStorage","getItem"],"sources":["/Users/nicolechan/Desktop/FYP_Test/itinerary/src/components/HistCard.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n        <div class=\"left\">\n          <div class=\"square\"></div>  \n        </div>\n      \n      <div class=\"right\">\n        <p>Username: {{ userName }}</p>\n        <p>{{ id }}</p>\n        <p>Plan name: {{ name }}</p>\n        <p><span style=\"font-size: 1rem\"><font-awesome-icon :icon=\"['fas', 'circle-user']\" /></span> Author: {{ author }}</p>\n        <p><span style=\"font-size: 1rem\"><font-awesome-icon :icon=\"['fas', 'people-robbery']\" /></span> Type: {{ type }}</p>\n        <p><span style=\"font-size: 1rem\"><font-awesome-icon :icon=\"['fas', 'users']\" /></span> Participants: {{ participants }}</p>\n        <p><span style=\"font-size: 1rem\"><font-awesome-icon :icon=\"['fas', 'calendar-days']\" /></span> From {{ start }} to {{ end }}</p>\n        <p>Ratings: <span v-for=\"n in rating\" :key=\"n\" style=\"margin-right:8px\"><font-awesome-icon :icon=\"['fas', 'star']\" /></span></p>\n        <p>View: {{ view }}</p>\n        <router-link :to=\"/history/+id\"><button class=\"btn view-btn\">View</button></router-link>\n\n                    <!-- Button trigger modal -->\n            <button\n              type=\"button\"\n              class=\"btn delete-btn \"\n              data-bs-toggle=\"modal\"\n              data-bs-target=\"#deleteModal\"\n              @click=\"setSelectedId(id)\"\n            >Delete {{ id }}\n            </button>\n\n            <!-- Modal -->\n            <div\n              class=\"modal fade\"\n              id=\"deleteModal\"\n              tabindex=\"-1\"\n              aria-labelledby=\"deleteModalLabel\"\n              aria-hidden=\"true\"\n            >\n              <div class=\"modal-dialog\">\n                <div class=\"modal-content\">\n                  <div class=\"modal-header\">\n                    <h5 class=\"modal-title\" id=\"deleteModalLabel\">\n                      Confirm delete? {{ selectedId }}\n                    </h5>\n                    <button\n                      type=\"button\"\n                      class=\"btn-close\"\n                      data-bs-dismiss=\"modal\"\n                      aria-label=\"Close\"\n                    ></button>\n                  </div>\n                  <div class=\"modal-body\">* Once deleted, the itinerary data cannot be retrieved.{{selectedId }}</div>\n                  <div class=\"modal-footer\">\n                    <button\n                      type=\"button\"\n                      class=\"btn btn-secondary\"\n                      data-bs-dismiss=\"modal\"\n                    >\n                      Cancel\n                    </button>\n                    <button type=\"button\" class=\"btn btn-danger\" @click=\"$emit('deleteItin', this.id)\">\n                      Delete\n                    </button>\n                  </div>\n                </div>\n              </div>\n            </div>\n      </div>\n    </div>\n</template>\n<script>\nexport default {\n  name: \"HistCard\",\n  props: {\n    author: String,\n    id: String,\n    name: String,\n    type: String,\n    participants: Number,\n    start: String,\n    end: String,\n    rating: Number,\n    view: String,\n  },\n  emit:[\n    \"deleteItin\"\n  ],\n  data(){\n    return{\n      userName: \"\",\n      selectedId: null\n    }\n  },\n  methods:{\n    setSelectedId(id) {\n      alert(id);\n      this.setSelectedId = id\n      alert(this.selectedId)\n    },\n  },\n  mounted(){\n    const un = localStorage.getItem(\"username\")\n    this.userName = un;\n  },\n};\n</script>\n<style scoped>\n.container {\n  width: 70%;\n  box-shadow: 0px 0px 10px 2px rgba(0, 0, 0, 0.1);\n  border-radius: 10px;\n  margin-top: 10px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 20px;\n  background-color: #fff;\n}\n.square {\n  background-color: burlywood;\n  background-image: url(\"@/assets/Images/Temporary_img.jpg\");\n  background-size: cover;\n  background-repeat: no-repeat;\n  background-position: center;\n  height: 250px;\n  width: 250px;\n  margin: 30px;\n  border-radius: 10px;\n}\n.left{\n    flex: 0 0 30%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n.right{\n    flex: 0 0 70%;\n}\n.view-btn{\n  background-color: #016a70;\n  color: #fff;\n  border-color: #016a70;\n  margin-right: 10px;\n}\n.view-btn:hover{\n  border-color: #FFDB64;\n  background-color: #FFDB64;\n  color: #a08843;\n  border-color: #a08843;\n}\n.delete-btn{\n  color: #016a70;\n  border-color: #016a70;\n  margin-right: 10px;\n}\n.delete-btn:hover{\n  background-color: #FA7070;\n  color: #fff;\n  border-color: #FA7070;\n}\n</style>\n"],"mappings":"AAqEA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,MAAM,EAAEC,MAAM;IACdC,EAAE,EAAED,MAAM;IACVH,IAAI,EAAEG,MAAM;IACZE,IAAI,EAAEF,MAAM;IACZG,YAAY,EAAEC,MAAM;IACpBC,KAAK,EAAEL,MAAM;IACbM,GAAG,EAAEN,MAAM;IACXO,MAAM,EAAEH,MAAM;IACdI,IAAI,EAAER;EACR,CAAC;EACDS,IAAI,EAAC,CACH,YAAW,CACZ;EACDC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;IACd;EACF,CAA