ities.Category===cat)\n    }\n  },\n  async created() {\n    const response = await axios\n      .get(\"/api/\")\n      .then((response) => (this.activities = response.data));\n\n    // const activities = response.data;\n    // this.activities = activities;\n\n    console.log(response);\n    // location.assign('/')\n  },\n};\n</script>\n\n<style scoped>\n.page {\n  margin: 10px;\n  margin: auto;\n}\n.card {\n  border-radius: 10px;\n  /* display: inline-block; */\n  margin-left: 15px;\n  margin-top: 20px;\n  /* height: 600px; */\n}\n.card:hover {\n  box-shadow: 0 0 2em 1em #f4aab9;\n}\n\n.scrolls {\n  display: flex;\n  flex-wrap: no-wrap;\n  overflow-x: auto;\n  margin: 20px;\n}\n</style>\n"],"mappings":"AAyBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE,CAAC,CAAC;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAC;IACNC,UAAUA,CAACC,GAAG,EAAC;MACX,OAAO,IAAI,CAACH,UAAU,CAACI,MAAM,CAAC,IAAI,CAACJ,UAAU,CAACK,QAAQ,KAAGF,GAAG;IAChE;EACF,CAAC;EACD,MAAMG,OAAOA,CAAA,EAAG;IACd,MAAMC,QAAO,GAAI,MAAMX,KAAI,CACxBY,GAAG,CAAC,OAAO,EACXC,IAAI,CAAEF,QAAQ,IAAM,IAAI,CAACP,UAAS,GAAIO,QAAQ,CAACR,IAAK,CAAC;;IAExD;IACA;;IAEAW,OAAO,CAACC,GAAG,CAACJ,QAAQ,CAAC;IACrB;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}