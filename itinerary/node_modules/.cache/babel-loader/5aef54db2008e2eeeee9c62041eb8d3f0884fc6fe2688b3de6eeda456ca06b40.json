{"ast":null,"code":"import NavBar from \"@/components/NavBar.vue\";\nimport PlanCard from \"@/components/PlanCard.vue\";\nimport SubplanCard from \"@/components/SubplanCard.vue\";\nimport PopupRateVue from \"@/components/PopupRate.vue\";\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nimport { ref } from \"vue\";\nexport default {\n  name: \"itinhistDetails\",\n  components: {\n    NavBar,\n    PlanCard,\n    SubplanCard,\n    PopupRateVue\n  },\n  data() {\n    return {\n      history: [],\n      dateDiff: 0,\n      itinArray: [],\n      itinSubArray: [],\n      dailyItin: [[{\n        itin: {\n          name: 1\n        }\n      }, {\n        itin: {\n          name: 1.1\n        }\n      }, {\n        itin: {\n          name: 1.2\n        }\n      }], [{\n        itin: {\n          name: 2\n        }\n      }], [{\n        itin: {\n          name: 3\n        }\n      }]]\n    };\n  },\n  methods: {\n    addDays(date, days) {\n      // Function to add Days\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleString();\n    },\n    weekdays(date, days) {\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleString(\"en-US\", {\n        weekday: \"long\"\n      });\n    },\n    async Rate(iid) {\n      const rateData = {\n        author: this.history.author,\n        name: this.history.name,\n        type: this.history.type,\n        participants: this.history.participants,\n        from: this.history.from,\n        to: this.history.to,\n        dailyItin: this.dailyItin,\n        is_public: this.history.is_public,\n        rate: parseInt(this.rate),\n        comment: this.comment\n      };\n      await axios.put(\"/api/rate/\" + iid, rateData).then(response => {\n        console.log(response.data);\n        alert(\"Successfully updated!\");\n      });\n      console.log(rateData);\n    }\n  },\n  mounted() {\n    const fetchData = async () => {\n      const route = useRoute();\n      const response = await axios.get(\"/api/history/\" + route.params.id).then(response => this.history = response.data);\n\n      // Calculate dateDiff based on the first and last items from the history array\n      const start = new Date(this.history.from).getTime();\n      const end = new Date(this.history.to).getTime();\n      const diff = end - start;\n      const dateDiff = Math.floor(diff / 86400000) + 1;\n      this.dateDiff = dateDiff;\n      console.log(response);\n      console.log(\"length: \" + this.history.dailyItin.length);\n      console.log(this.history.dailyItin[2]);\n      this.itinArray = this.history.dailyItin;\n      console.log(\"length2: \" + this.itinArray.length);\n      console.log(this.history.dailyItin[0]);\n    };\n    fetchData();\n  },\n  setup() {\n    const rate = ref();\n    const comment = ref(\"\");\n    const popupTriggers = ref({\n      buttonTrigger: false\n    });\n    const togglePopup = trigger => {\n      popupTriggers.value[trigger] = !popupTriggers.value[trigger];\n    };\n    return {\n      popupTriggers,\n      togglePopup,\n      rate,\n      comment\n    };\n  }\n};","map":{"version":3,"names":["NavBar","PlanCard","SubplanCard","PopupRateVue","axios","useRoute","ref","name","components","data","history","dateDiff","itinArray","itinSubArray","dailyItin","itin","methods","addDays","date","days","result","Date","setDate","getDate","toLocaleString","weekdays","weekday","Rate","iid","rateData","author","type","participants","from","to","is_public","rate","parseInt","comment","put","then","response","console","log","alert","mounted","fetchData","route","get","params","id","start","getTime","end","diff","Math","floor","length","setup","popupTriggers","buttonTrigger","togglePopup","trigger","value"],"sources":["/Users/nicolechan/Desktop/FYP_Test/itinerary/src/pages/ItinhistDetails.vue"],"sourcesContent":["<template>\n  <div>\n    <NavBar />\n    <h1>Itinerary History Details</h1>\n    <div class=\"scrolls\">\n      <PlanCard\n        v-for=\"(n, i) in dateDiff\"\n        :key=\"n\"\n        :n=\"n\"\n        :start=\"addDays(this.history.from, i)\"\n        :daysOfWeek=\"weekdays(this.history.from, i)\"\n      >\n        <div v-for=\"(row, rowIndex) in itinArray[i]\" :key=\"rowIndex\">\n          <div v-for=\"(element, columnIndex) in row\" :key=\"columnIndex\">\n            <SubplanCard :actName=\"element.act_name\" :memo=\"element.memo\">\n            </SubplanCard>\n          </div>\n        </div>\n      </PlanCard>\n    </div>\n\n    <div>\n      <button\n        class=\"btn rate-btn\"\n        @click.prevent=\"() => togglePopup('buttonTrigger')\"\n      >\n        <span style=\"font-size: 1.5em; margin-right: 5px\">\n          <font-awesome-icon :icon=\"['fas', 'star']\" /> </span\n        >Rate\n      </button>\n      <div>\n        <p v-for=\"n in this.history.rate\" :key=\"n\" style=\"display: inline; margin: 5px;\">\n          <font-awesome-icon :icon=\"['fas', 'star']\" />\n        </p>\n      </div>\n\n      <p><span style=\"margin: 5px\"><font-awesome-icon :icon=\"['fas', 'comment-dots']\" /></span>{{ this.history.comment }}</p>\n    </div>\n\n    <PopupRateVue\n      v-if=\"popupTriggers.buttonTrigger\"\n      :togglePopup=\"() => togglePopup('buttonTrigger')\"\n      v-model:rate=\"rate\"\n      v-model:comment=\"comment\"\n      @rate=\"Rate(this.history._id)\"\n    />\n\n    <!-- <p v-for=\"(itin, i) in this.history.dailyItin\" :key=\"i\">\n      {{ itin }}\n    </p> -->\n    <!-- <p>{{ history }}</p> -->\n    <p>dateDiff: {{ dateDiff }}</p>\n    <p></p>\n  </div>\n</template>\n\n<script>\nimport NavBar from \"@/components/NavBar.vue\";\nimport PlanCard from \"@/components/PlanCard.vue\";\nimport SubplanCard from \"@/components/SubplanCard.vue\";\nimport PopupRateVue from \"@/components/PopupRate.vue\";\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nimport { ref } from \"vue\";\n\nexport default {\n  name: \"itinhistDetails\",\n  components: {\n    NavBar,\n    PlanCard,\n    SubplanCard,\n    PopupRateVue,\n  },\n  data() {\n    return {\n      history: [],\n      dateDiff: 0,\n      itinArray: [],\n      itinSubArray: [],\n      dailyItin: [\n        [\n          { itin: { name: 1 } },\n          { itin: { name: 1.1 } },\n          { itin: { name: 1.2 } },\n        ],\n        [{ itin: { name: 2 } }],\n        [{ itin: { name: 3 } }],\n      ],\n    };\n  },\n  methods: {\n    addDays(date, days) {\n      // Function to add Days\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleString();\n    },\n    weekdays(date, days) {\n      var result = new Date(date);\n      result.setDate(result.getDate() + days);\n      return result.toLocaleString(\"en-US\", { weekday: \"long\" });\n    },\n    async Rate(iid) {\n      const rateData = {\n        author: this.history.author,\n        name: this.history.name,\n        type: this.history.type,\n        participants: this.history.participants,\n        from: this.history.from,\n        to: this.history.to,\n        dailyItin: this.dailyItin,\n        is_public: this.history.is_public,\n        rate: parseInt(this.rate),\n        comment: this.comment,\n      };\n\n      await axios.put(\"/api/rate/\" + iid, rateData).then((response) => {\n        console.log(response.data);\n        alert(\"Successfully updated!\");\n      });\n      console.log(rateData);\n    },\n  },\n  mounted() {\n    const fetchData = async () => {\n      const route = useRoute();\n      const response = await axios\n        .get(\"/api/history/\" + route.params.id)\n        .then((response) => (this.history = response.data));\n\n      // Calculate dateDiff based on the first and last items from the history array\n      const start = new Date(this.history.from).getTime();\n      const end = new Date(this.history.to).getTime();\n      const diff = end - start;\n      const dateDiff = Math.floor(diff / 86400000) + 1;\n      this.dateDiff = dateDiff;\n      console.log(response);\n      console.log(\"length: \" + this.history.dailyItin.length);\n      console.log(this.history.dailyItin[2]);\n\n      this.itinArray = this.history.dailyItin;\n      console.log(\"length2: \" + this.itinArray.length);\n      console.log(this.history.dailyItin[0]);\n    };\n    fetchData();\n  },\n  setup() {\n    const rate = ref();\n    const comment = ref(\"\");\n\n    const popupTriggers = ref({\n      buttonTrigger: false,\n    });\n\n    const togglePopup = (trigger) => {\n      popupTriggers.value[trigger] = !popupTriggers.value[trigger];\n    };\n\n    return {\n      popupTriggers,\n      togglePopup,\n      rate,\n      comment,\n    };\n  },\n};\n</script>\n<style scoped>\n.rate-btn {\n  border-color: #016a70;\n  color: #016a70;\n}\n.rate-btn :hover {\n  border-color: #016a70;\n  background-color: #016a70;\n  color: #ffffdd;\n}\n.scrolls {\n  display: flex;\n  flex-wrap: no-wrap;\n  overflow-x: auto;\n  margin: 20px;\n}\n</style>\n"],"mappings":"AAyDA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,YAAW,MAAO,4BAA4B;AACrD,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVR,MAAM;IACNC,QAAQ;IACRC,WAAW;IACXC;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,CAAC;MACXC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE,CACT,CACE;QAAEC,IAAI,EAAE;UAAER,IAAI,EAAE;QAAE;MAAE,CAAC,EACrB;QAAEQ,IAAI,EAAE;UAAER,IAAI,EAAE;QAAI;MAAE,CAAC,EACvB;QAAEQ,IAAI,EAAE;UAAER,IAAI,EAAE;QAAI;MAAE,CAAC,CACxB,EACD,CAAC;QAAEQ,IAAI,EAAE;UAAER,IAAI,EAAE;QAAE;MAAE,CAAC,CAAC,EACvB,CAAC;QAAEQ,IAAI,EAAE;UAAER,IAAI,EAAE;QAAE;MAAE,CAAC,CAAC;IAE3B,CAAC;EACH,CAAC;EACDS,OAAO,EAAE;IACPC,OAAOA,CAACC,IAAI,EAAEC,IAAI,EAAE;MAClB;MACA,IAAIC,MAAK,GAAI,IAAIC,IAAI,CAACH,IAAI,CAAC;MAC3BE,MAAM,CAACE,OAAO,CAACF,MAAM,CAACG,OAAO,CAAC,IAAIJ,IAAI,C