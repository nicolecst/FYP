{"ast":null,"code":"// import PopupAct from '@/components/PopupAct.vue';\n\nexport default {\n  name: \"PopupForm\",\n  // props: [\"togglePopup\", \"n\", \"act\", \"day\", \"sTime\", \"eTime\", \"memo\"],\n  props: {\n    togglePopup: Function,\n    n: String,\n    act: String,\n    location: String,\n    day: Number,\n    sTime: String,\n    eTime: String,\n    memo: String\n  },\n  emit: [\"update:act\", \"update:location\", \"update:Day\", \"update:sTime\", \"update:eTime\", \"update:Memo\", \"clicked\"],\n  methods: {\n    clicked() {\n      console.log(123);\n    }\n  }\n  //   component:{\n  //     PopupAct\n  //   }\n};","map":{"version":3,"names":["name","props","togglePopup","Function","n","String","act","location","day","Number","sTime","eTime","memo","emit","methods","clicked","console","log"],"sources":["/Users/nicolechan/Desktop/FYP_Test/itinerary/src/components/PopupForm.vue"],"sourcesContent":["<template>\n  <div class=\"popup\">\n    <div class=\"popup-inner\">\n      <div class=\"formHeader\">\n        <div>\n          <h1>Add activities</h1>\n        </div>\n        <div>\n          <button class=\"btn close\" @click.prevent=\"togglePopup()\">\n            <span style=\"font-size: 1.5rem\"\n              ><font-awesome-icon icon=\"fa-solid fa-circle-xmark\"\n            /></span>\n          </button>\n        </div>\n      </div>\n\n      <form action=\"\">\n        <div class=\"row mt-2\">\n          <label for=\"\">Activity</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            :value=\"act\"\n            @input=\"$emit('update:act', $event.target.value)\"\n          />\n          <!-- <p>{{ act }}</p> -->\n          <!-- <button>Browse</button> -->\n        </div>\n        <div class=\"row mt-2\">\n          <label for=\"\">Location</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            :value=\"location\"\n            @input=\"$emit('update:location', $event.target.value)\"\n          />\n          <!-- <p>{{ location }}</p> -->\n        </div>\n        <slot></slot>\n\n        <div class=\"row mt-2\">\n          <div class=\"col md-4\">\n            <label for=\"\">Day</label>\n            <select\n              name=\"\"\n              id=\"\"\n              class=\"form-control\"\n              required\n              :value=\"day\"\n              @input=\"$emit('update:Day', $event.target.value)\"\n            >\n              <option v-for=\"i in n\" :key=\"i\">{{ i }}</option>\n            </select>\n            <!-- <p>{{ day }}</p> -->\n          </div>\n\n          <div class=\"col md-4\">\n            <label for=\"\">Start</label>\n            <input\n              type=\"time\"\n              name=\"\"\n              id=\"\"\n              class=\"form-control\"\n              :value=\"sTime\"\n              @input=\"$emit('update:sTime', $event.target.value)\"\n            />\n          </div>\n          <div class=\"col md-4\">\n            <label for=\"\">End</label>\n            <input\n              type=\"time\"\n              name=\"\"\n              id=\"\"\n              class=\"form-control\"\n              :value=\"eTime\"\n              @input=\"$emit('update:eTime', $event.target.value)\"\n            />\n          </div>\n\n          <!-- <p>{{ sTime }} {{ eTime }}</p> -->\n        </div>\n        <div class=\"row mt-2\">\n          <label for=\"\">Memo</label>\n          <textarea\n            type=\"text\"\n            name=\"\"\n            id=\"\"\n            class=\"form-control\"\n            :value=\"memo\"\n            @input=\"$emit('update:Memo', $event.target.value)\"\n          ></textarea>\n        </div>\n        <!-- <p>{{ memo }}</p> -->\n\n        <div class=\"row mt-2\">\n          <button class=\"btn add-btn\" @click.prevent=\"$emit('clicked')\">\n            Add\n          </button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\n// import PopupAct from '@/components/PopupAct.vue';\n\nexport default {\n  name: \"PopupForm\",\n  // props: [\"togglePopup\", \"n\", \"act\", \"day\", \"sTime\", \"eTime\", \"memo\"],\n  props: {\n    togglePopup: Function,\n    n: String,\n    act: String,\n    location: String,\n    day: Number,\n    sTime: String,\n    eTime: String,\n    memo: String,\n  },\n  emit: [\n    \"update:act\",\n    \"update:location\",\n    \"update:Day\",\n    \"update:sTime\",\n    \"update:eTime\",\n    \"update:Memo\",\n    \"clicked\",\n  ],\n  methods: {\n    clicked() {\n      console.log(123);\n    },\n  },\n  //   component:{\n  //     PopupAct\n  //   }\n};\n</script>\n\n<style scoped>\n.popup {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  z-index: 99;\n  background-color: rgba(0, 0, 0, 0.2);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 10px;\n}\n.popup-inner {\n  background-color: #fff;\n  padding: 30px;\n  border-radius: 10px;\n  width: 60%;\n}\n.formHeader {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin: 10px;\n}\n.add-btn{\n  border-radius: 5px;\n  background-color: #016a70;\n  color: #fff;\n}\n.add-btn:hover{\n  border-color: #ffdb64;\n  background-color: #ffdb64;\n  color: #a08843;\n  border-color: #a08843;\n}\n</style>\n"],"mappings":"AAyGA;;AAEA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjB;EACAC,KAAK,EAAE;IACLC,WAAW,EAAEC,QAAQ;IACrBC,CAAC,EAAEC,MAAM;IACTC,GAAG,EAAED,MAAM;IACXE,QAAQ,EAAEF,MAAM;IAChBG,GAAG,EAAEC,MAAM;IACXC,KAAK,EAAEL,MAAM;IACbM,KAAK,EAAEN,MAAM;IACbO,IAAI,EAAEP;EACR,CAAC;EACDQ,IAAI,EAAE,CACJ,YAAY,EACZ,iBAAiB,EACjB,YAAY,EACZ,cAAc,EACd,cAAc,EACd,aAAa,EACb,SAAS,CACV;EACDC,OAAO,EAAE;IACPC,OAAOA,CAAA,EAAG;MACRC,OAAO,CAACC,GAAG,CAAC,GAAG,CAAC;IAClB;EACF;EACA;EACA;EACA;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}