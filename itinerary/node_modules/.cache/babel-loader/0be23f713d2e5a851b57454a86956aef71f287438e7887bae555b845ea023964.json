turn {\n      activities: [],\n    };\n  },\n  async created() {\n    const response = await axios\n      .get(\"/api/\")\n      .then((response) => (this.activities = response.data));\n\n    // const activities = response.data;\n    // this.activities = activities;\n\n    console.log(response);\n    // location.assign('/')\n  },\n};\n</script>\n\n<style scoped>\n.page {\n  margin: 10px;\n  margin: auto;\n}\n.card {\n  border-radius: 10px;\n  /* display: inline-block; */\n  margin-left: 15px;\n  margin-top: 20px;\n  /* height: 600px; */\n}\n.card:hover {\n  box-shadow: 0 0 2em 1em #f4aab9;\n}\n\n.scrolls {\n  display: flex;\n  flex-wrap: no-wrap;\n  overflow-x: auto;\n  margin: 20px;\n}\n</style>\n"],"mappings":"AA0BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE,CAAC,CAAC;EACdC,OAAO,EAAC;IACNC,MAAMA,CAACC,GAAG,EAAC;MACP,OAAO,IAAI,CAACC,UAAU,CAACC,MAAM,CAAEC,CAAC,IAAIA,CAAC,CAACC,QAAO,KAAMJ,GAAG;IAC1D;EAEF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLJ,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACD,MAAMK,OAAOA,CAAA,EAAG;IACd,MAAMC,QAAO,GAAI,MAAMZ,KAAI,CACxBa,GAAG,CAAC,OAAO,EACXC,IAAI,CAAEF,QAAQ,IAAM,IAAI,CAACN,UAAS,GAAIM,QAAQ,CAACF,IAAK,CAAC;;IAExD;IACA;;IAEAK,OAAO,CAACC,GAAG,CAACJ,QAAQ,CAAC;IACrB;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}